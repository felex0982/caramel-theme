{
	"name": "caramel",
	"description": "Minimalistik Portfolio Theme for Wordpress",
	"type": "wordpress-theme",
	"license": "GPL-2.0-only",
	"minimum-stability": "stable",
	"keywords": [
		"wordpress",
		"theme",
		"bootstrap"
	],
	"homepage": "https://github.com/felex0982/caramel-theme",
	"require": {
		"composer/installers": "^1.9",
		"php": ">=5.6"
	},
	"require-dev": {
		"roave/security-advisories": "dev-master",
		"wp-coding-standards/wpcs": "^2.3",
		"wptrt/wpthemereview": "*",
		"dealerdirect/phpcodesniffer-composer-installer": "^0.6",
		"php-parallel-lint/php-parallel-lint": "^1.2"
	},
	"scripts": {
		"check:cs": ["@php ./vendor/squizlabs/php_codesniffer/bin/phpcs"],
		"fix:cs": ["@php ./vendor/squizlabs/php_codesniffer/bin/phpcbf"],
		"lint:php": ["@php ./vendor/bin/parallel-lint --exclude .git --exclude vendor ."]
	},
	"support": {
		"issues": "https://github.com/felex0982/caramel-theme/issues",
		"source": "https://github.com/felex0982/caramel-theme"
	}
}